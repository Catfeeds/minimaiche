page {
  font-size: 28rpx;
  font-family: "微软雅黑";
}
.appoin_title {
  font-size: 33rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.appoin_name view {
  color: #ADADAD;
  margin-bottom: 10rpx;
  margin-top: 20rpx;
}
.appoin_name input {
  border: 2px solid #F5F5F5;
  padding-left: 15rpx;
}
.appoin_name {
  border-bottom: 1px solid #D9D9D9;
  padding-bottom: 10rpx;
  padding: 0 20rpx 20rpx;
  box-sizing: border-box;
}
.button {
  width: 50%;
  margin-top: 50rpx;
}